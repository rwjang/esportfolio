header {

	.navbar {
		background:white;
		border-bottom:none;
		border-top:8px solid dark-blue;
		-webkit-box-shadow: 0px 3px 5px 0px rgba(0,0,0,0.08);
		-moz-box-shadow: 0px 3px 5px 0px rgba(0,0,0,0.08);
		box-shadow: 0px 3px 5px 0px rgba(0,0,0,0.08);
    @media (min-width: 768px) {
			
		}
		.container {
			margin-top:0px;
			margin-bottom:0px;
			padding-right: 0px;
    	padding-left: 0px;	
			-webkit-transition: margin .3s ease;
		  -moz-transition: margin .3s ease;
		  -o-transition: margin .3s ease;
		  @media (min-width: 992px) {
				padding-right: 15px;
    		padding-left: 15px;	
    		margin-bottom:5px;
    		margin-top:5px;
    	}
		}
		&.skinnynav {
                    .vert-left {
                        top: 8px;
                        height: 45px;
                    }
                    .head-text {
                        font-size: 14px;
                        line-height: 1.8rem;
                    }
			padding:0px 0;
			-webkit-transition: all .3s ease;
		  -moz-transition: all .3s ease;
		  -o-transition: all .3s ease;
		  transition: all .3s ease;
		  .btn {
				&.yellow {
					&.signin {
						margin-top:13px;
						@media (min-width: 480px) {
							margin-top:15px;
			    	}
						@media (min-width: 992px) {
							margin-top:15px;
			    	}
					}
				}
			}
			.navbar-brand {
				padding-top:15px;
				padding-bottom:15px;
				-webkit-transition: all .3s ease;
			  -moz-transition: all .3s ease;
			  -o-transition: all .3s ease;
			  transition: all .3s ease;
				svg {
					width:auto;
					height:22px!important;
					-webkit-transition: all .3s ease;
				  -moz-transition: all .3s ease;
				  -o-transition: all .3s ease;
				  transition: all .3s ease;
				  @media (min-width: 480px) {
						height:30px!important;
						width:auto;
			    }
				  @media (min-width: 992px) {
						height:30px!important;
						width:auto;
			    }
				}
			}
			.container {
				margin-top:0px;
				margin-bottom:0px;
			}
			.navbar-collapse {
				float:right;
				margin-top:4px;
				-webkit-transition: all .3s ease;
			  -moz-transition: all .3s ease;
			  -o-transition: all .3s ease;
			  transition: all .3s ease;
			}
			.navbar-nav {
                               
				li {
					.dropdown-menu {
						top:59px;
					}
				}
			}
		}
		.navbar-collapse {
			float:right;
			margin-top:0px;
			-webkit-transition: all .3s ease;
		  -moz-transition: all .3s ease;
		  -o-transition: all .3s ease;
		  transition: all .3s ease;
		  width:100%;
		  background: dark-blue;
		  border:none;
		  max-height: inherit;
		  padding:0;
		  position:absolute;
			top:52px;
			left:0;
			right:0;
			@media (min-width: 480px) {
				top:60px;
			}
		  @media (min-width: 768px) {
		  	display:none!important;
		  	top:60px;
		  	width:750px;
		  	margin:0px auto;
		  }
		  @media (min-width: 992px) {
		  	position:static;
		  	width:auto;
		  	display:block!important;
		  	background: white;
		  	margin-top:8px;
		  }
		  &.collapsing {
		  	display:block!important;
		  }
		  &.in {
		  	@media (min-width: 768px) {
		  		display:block!important;
			  }
			  @media (min-width: 992px) {
			  	
			  }
		  }
		}
		.navbar-brand {
			height:auto;
			-webkit-transition: all .3s ease;
		  -moz-transition: all .3s ease;
		  -o-transition: all .3s ease;
		  transition: all .3s ease;
		  padding-right:0;
			svg {
				-webkit-transition: all .3s ease;
			  -moz-transition: all .3s ease;
			  -o-transition: all .3s ease;
			  transition: all .3s ease;
				height:22px!important;
				width:auto;
				display:block;
			}
			@media (min-width: 480px) {
				svg {
					height:30px!important;
					width:auto;
				}
			}
			@media (min-width: 992px) {
				padding-left:15px;
				padding-right:15px;
				svg {
					height:40px!important;
					width:auto;
				}
			}
		}
		.navbar-header {
			width:100%;
			float:none;
			margin-right: 0px;
                        margin-left: 0px;
                        .desktop-nav {
                            display: block;
                            @media (max-width: 992px) {
                               display: none; 
                            }
                        }
			@media (min-width: 992px) {
				margin-right: -15px;
                                margin-left: -15px;
			}
                        
		}
		.navbar-toggle {
			border:none;
			float:right;
			margin-right:15px;
			padding-right:0;
			@media (min-width: 480px) { 
				margin-top:12px;
				margin-left:15px;
			}
			@media (min-width: 768px) { 
				display:block;
				margin-right:0;
				padding-right:0;
			}
			@media (min-width: 992px) {
				margin-right:15px;
				display:none;
				margin-right:15px;
				padding-right:10px;
			}
			.icon-bar {
				background:gray;
			}
			&:hover, &:focus {
				background:none;
				.icon-bar {
					background:gray;
				}
			}
			&:active {
				
			}
		}
		.navbar-nav {
			margin:0;
			float:none;
			@media (min-width: 768px) {
				float:none;
			}
			@media (min-width: 992px) {
				float:left;
                                .mobile-nav {
                                    display: none;
                                    @media (max-width: 992px) {
                                        display: block;
                                    }    
                                }    
			}
			li {
				
				@media (min-width: 768px) {
					float:none;
				}
				@media (min-width: 992px) {
					float:left;
				}
				&:last-child {
					a {
						border:none;
						&:hover, &:active, &:focus {
							border:none;
						}
					}
				}
				a {
					font-size: 15px;
					font-weight: 500;
					color:white;
					border-bottom:1px solid #2D5A7F;
					&:hover, &:active, &:focus {
						color:yellow;
						border-bottom:1px solid #2D5A7F;
					}
					@media (min-width: 992px) {
						font-size: 15px;
						font-weight: 500;
						color:gray;
						border:none;
						&:hover, &:active, &:focus {
							color:yellow;
							border:none;
						}
					}
				}
				&.open {
					background:none;
					color:yellow;
					a {
						background:none;
						color:yellow;
						&:focus, &:hover {
							background:none;
							color:yellow;
						}
					}
				}
				&.active {
					a {
						color:yellow;
						&:hover {
							color:yellow;
						}
					}
				}
				.dropdown-menu {	
					padding: 10px 0;
					background: #2D5A7F;
					border-radius: 0;
					-webkit-transition: all .3s ease;
				  -moz-transition: all .3s ease;
				  -o-transition: all .3s ease;
				  transition: all .3s ease;
				  position:static;
				  li {
				  	float:none;
				  }
					a {
						color:white!important;
						border:none!important;
					}
					@media (min-width: 768px) {
						position:static;
						width:100%;
						float:none;
						-webkit-box-shadow: none;
    				box-shadow: none;
    				border-top:none;
					}
					@media (min-width: 992px) { 
						padding: 5px 0;
						background: dark-blue;
						color:white;
						border:none;
						padding-top:12px;
						padding-bottom:12px;
						top:67px;
						width:auto;
						position:absolute;
						-webkit-box-shadow: 0 6px 12px rgba(0,0,0,.175);
   					box-shadow: 0 6px 12px rgba(0,0,0,.175);
						&:before {
							width: 0;
					    height: 0;
					    border-left: 7px solid transparent;
					    border-right: 7px solid transparent;
					    border-bottom: 7px solid dark-blue;
					    content: '';
					    position: absolute;
					    top: -7px;
					    left: 30px;
						}
					}
				}
			}
		}

		.signin {
			@media (min-width: 992px) {
				float:left;
			}
			&.active-signin {
				padding-bottom:45px;
				margin-bottom:-45px;
				-webkit-border-bottom-right-radius: 0px;
				-webkit-border-bottom-left-radius: 0px;
				-moz-border-radius-bottomright: 0px;
				-moz-border-radius-bottomleft: 0px;
				border-bottom-right-radius: 0px;
				border-bottom-left-radius: 0px;
			}
		}

		.signin-menu {
			background:yellow;
			padding:20px 0;
			color:white;
			position:absolute;
			left:0;
			right:0;
			top:52px;
			@media (min-width: 480px) {
				top:60px;
			}
			@media (min-width: 768px) {
				width: 750px;
				margin:0 auto;
			}
			@media (min-width: 992px) {
				position:static;
				width:auto;
			}
			.signin-left {
				border-bottom:1px solid white;
				padding-bottom:15px;
				margin-bottom:15px;
				@media (min-width: 992px) {
					padding-left:0;
					padding-right:0;
					padding-bottom:0px;
					margin-bottom:0px;
					border-bottom:none;
					border-right:1px solid white;
				}
			}
			.signin-right {
				@media (min-width: 992px) {
					padding-right:0;
					padding-left:35px;
				}
			}
			form {
				margin:0;
				.row {
					margin-bottom:0;
				}
			}
			h3 {
				margin-top:5px;
				margin-bottom:10px;
				font-size:25px;
			}
			input {
				width:100%;
				border:none;
				color: #000000;
				margin-bottom:10px;
				@media (min-width: 992px) {
					margin-bottom:0;
				}
				&.form-control {
					height: 38px;
                  color: black;
				}
				&.btn {
					color:white;
					margin-top:0;
				}
			}
			p {
				color:white;
				font-size: 12px;
				clear:both;
				margin-bottom: 0;
                                font-weight: 400;
				a {
					color:white;
				}
			}
		}

	}
        .vert-left {
            display: inline-block;
            position: relative;
            margin-left: .7rem;
            margin-right: .7rem;
            padding-left: 1rem;
            padding-right: .5rem;
            top: 10px;
            height: 55px;
            border-left: 1px solid light-gray;
            float: left;
            padding-top: .5rem;  
            @media (max-width: 992px) {
                    display: none;
            }
        }
        .auth-menu {
            cursor: pointer;
            &:hover {
                color: yellow;
            }
            strong {
                font-size: 16px;
                font-weight: normal;
            }
            .profile-photo {
                height: 35px;
                border-radius: 18px;
                margin-top: -7px;
            }
            .dropdown-menu {	
                padding: 10px 0;
                background: #2D5A7F;
                border-radius: 0;
                -webkit-transition: all .3s ease;
                -moz-transition: all .3s ease;
                -o-transition: all .3s ease;
                transition: all .3s ease;
                position:static;
                li {
                    float:none; 
                }
                a {
                    color:white!important;
                    border:none!important;
                    &:hover {
                        background: dark-blue;
                    }
                }
                @media (min-width: 768px) {
                    position:static;
                    width:100%;
                    float:none;
                    -webkit-box-shadow: none;
                    box-shadow: none;
                    border-top:none;
                }
                @media (min-width: 992px) { 
                    padding: 5px 0;
                    background: dark-blue;
                    color:white;
                    border:none;
                    padding-top:12px;
                    padding-bottom:12px;
                    top:40px;
                    width:auto;
                    position:absolute;
                    -webkit-box-shadow: 0 6px 12px rgba(0,0,0,.175);
                    box-shadow: 0 6px 12px rgba(0,0,0,.175);
                    &:before {
                        width: 0;
                        height: 0;
                        border-left: 7px solid transparent;
                        border-right: 7px solid transparent;
                        border-bottom: 7px solid dark-blue;
                        content: '';
                        position: absolute;
                        top: -7px;
                        left: 30px;
                    }
                }
            }
    }
    .profile-photo {
        height: 35px;
        border-radius: 18px;
        margin-top: -7px;
    }
}